{
  "xlsx": {
    "head": 2,
    "src": "../Wood_Docs/xlsx/**/[^~$]*.xlsx",
    "dest": "../Resources/xdata"
  },
  "ts": false,
  "esc" : true,
  "xml" : false,
  "json": {
    "uglify": true
  },
  "plugins": {
    "ttf" : {
      "extra":"ttf-text",
      "src": "../Wood_Docs/fonts",
      "dest": "../Resources/fonts"
    }
  }
}